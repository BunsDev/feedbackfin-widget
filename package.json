{
  "name": "feedbackfin",
  "version": "0.1.1",
  "homepage": "https://feedbackfin.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowyio/feedbackfin.git"
  },
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "node ./generate-form-html && microbundle --define process.env.NODE_ENV=production",
    "dev": "node ./generate-form-html && microbundle watch --define process.env.NODE_ENV=production",
    "prepare": "node ./generate-form-html && microbundle --define process.env.NODE_ENV=production"
  },
  "devDependencies": {
    "@floating-ui/dom": "^0.4.2",
    "focus-trap": "^6.7.3",
    "html-minifier": "^4.0.0",
    "microbundle": "^0.14.2"
  }
}
